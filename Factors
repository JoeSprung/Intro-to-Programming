import java.util.Scanner;
public class Factors
{
    public static void main(String [] args)
    {
    Scanner in = new Scanner (System.in);
    System.out.println("Please type a positive number");
    int num = in.nextInt();
    for(int i = 1; 1 <= num / 2; i++)
    {
      if(num % i == 0)
      {
      System.out.print(i + ", ");
      }
    } 
}
}
